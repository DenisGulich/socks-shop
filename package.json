{
  "name": "magazine",
  "version": "1.0.0",
  "description": "My breathtaking Nuxt.js project",
  "author": "Denys Hulych",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "json-server": "node ./api/start",
    "json-server:watch": "nodemon --watch ./api ./api/start",
    "test:unit": "jest",
    "test": "yarn test:unit --coverage"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.8.5",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/svg-sprite": "^0.4.10",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "intersection-observer": "^0.7.0",
    "lazysizes": "^5.2.0",
    "nodemon": "^2.0.2",
    "nuxt": "^2.0.0",
    "objectFitPolyfill": "^2.3.0",
    "vue-i18n": "^8.15.5",
    "vue-scrollto": "^2.17.1",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
